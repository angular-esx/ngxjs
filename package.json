    "@angular/router": "4.0.1",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "typescript": "2.3.3",
    "zone.js": "0.8.5"
  },
  "peerDependencies": {
    "lerna": "2.0.0-beta.31"
  },
