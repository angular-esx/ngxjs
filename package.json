    "start": "npm run build-dll && cross-env BUILD_ENV=development webpack-dev-server --config $npm_package_envts_webpack_config --hot --inline",
    "start-dashboard": "npm run build-dll && cross-env BUILD_ENV=development:dashboard webpack-dashboard -- webpack-dev-server --config $npm_package_envts_webpack_config --hot --inline",
