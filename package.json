  "envts": {
    "webpack_config": "./_configs/_webpack/index.ts",
    "webpack_dll_config": "./_configs/_webpack/index.dll.ts"
  },
  "scripts": {
    "build-dll": "cross-env BUILD_ENV=development webpack --config $npm_package_envts_webpack_dll_config --progress",
    "build": "npm run build-dll && cross-env BUILD_ENV=development webpack --config $npm_package_envts_webpack_config --progress",
    "start": "npm run build-dll && cross-env BUILD_ENV=development webpack-dev-server --config $npm_package_envts_webpack_config --hot --inline --open",
    "start-dashboard": "npm run build-dll && cross-env BUILD_ENV=development:dashboard webpack-dashboard -- webpack-dev-server --config $npm_package_envts_webpack_config --hot --inline --open",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "@angular/animations": "4.0.1",
    "@angular/common": "4.0.1",
